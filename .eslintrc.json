{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:node/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": "error",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "no-console": 1
  },
  "env": {
    "jest": true
  }
}
